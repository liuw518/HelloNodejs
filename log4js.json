{
    "appenders":
        [
            {
                "type":"console",
                "category":"console"
            },
            {
                "category": [ "logFile" ],
                "type": "dateFile",
                "filename": "logs/date",
                "alwaysIncludePattern": true,
                "pattern": "-yyyy-MM-dd-hh.log"
            }
        ],
    "replaceConsole": false,
    "levels":
    {
        "logFile":"AUTO",
        "console":"ALL"
    }
}